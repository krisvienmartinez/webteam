  body#page-top(data-spy='scroll', data-target='.navbar-fixed-top')
    // Navigation
    nav.navbar.navbar-inverse.navbar-fixed-top(role='navigation')
      .container
        .navbar-header.page-scroll
          button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-ex1-collapse')
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand.page-scroll(href='#page-top')
            img(src='img/logo.png', height='18', width='140')
        // Collect the nav links, forms, and other content for toggling
        .collapse.navbar-collapse.navbar-ex1-collapse.navHeaderCollapse
          ul.nav.navbar-nav.navbar-right
            // Hidden li included to remove active class from about link when scrolled up past about section
            li.hidden
              a.page-scroll(href='#page-top')
            li
              a.page-scroll(href='#about') About
            li
              a.page-scroll(href='#services') Services
            li.dropdown
              a.page-scroll(href='#events') Events
            li
              a(href='#onlinereg', data-toggle='modal') Online Registration
            li
              a.page-scroll(href='#contacts') Contact
    #onlinereg.modal.fade(role='dialog')
      .modal-dialog
        .modal-content
          form.form-horizontal
            .modal-header
              h2 Online Registration
            .modal-body
              .form-group
                label.col-lg-2.control-label(for='signup-name') Name:
                .col-lg-10
                  input#signup-name.form-control(type='text', placeholder='Full Name')
                br
              .form-group
                label.col-lg-2.control-label(for='signup-email') Email:
                .col-lg-10
                  input#signup-email.form-control(type='email', placeholder='Email')
                br
              .form-group
                label.col-lg-2.control-label(for='signup-number') Contact:
                .col-lg-5
                  input#signup-number.form-control(type='number', placeholder='Number')
                br
              .form-group
                label.col-lg-2.control-label(for='signup-age') Age:
                .col-lg-2
                  input#signup-age.form-control(type='number', placeholder='Age')
                br
              .form-group
                label.col-lg-2.control-label(for='signup-gender') Gender:
                .col-lg-3
                  select#signup-gender.form-control(placeholder='Gender')
                    option Male
                    option Female
              .form-group
                label.col-lg-2.control-label(for='signup-address') Address:
                .col-lg-10
                  input#signup-address.form-control(type='text', placeholder='Address')
                br
              .form-group
                label.col-lg-2.control-label(for='signup-event') Event:
                .col-lg-10
                  select#signup-event.form-control(placeholder='chooseanevent')
                    option Events
                    option Los Angeles
                    option Tokyo
                    option New York
                    option London
            .modal-footer
              button.btn.btn-primary(type='submit') Submit
              a.btn.btn-default(data-dismiss='modal') Close
